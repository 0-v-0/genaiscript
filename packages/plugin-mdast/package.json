{
  "name": "@genaiscript/plugin-mdast",
  "private": false,
  "version": "2.2.0",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.d.*ts.map",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@genaiscript/core": "workspace:*",
    "@inquirer/prompts": "catalog:",
    "@slorber/remark-comment": "^1.0.0",
    "@types/mdast": "catalog:",
    "debug": "catalog:",
    "mdast": "^3.0.0",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "remark-directive": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-github": "^12.0.0",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/debug": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "tshy": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "catalog:"
  },
  "scripts": {
    "build": "npm run clean && tshy",
    "clean": "rimraf dist",
    "format:check": "prettier --config ../../.prettierrc.json --ignore-path ../../.prettierignore --check  \"src/**/*.{ts,cts,mts}\" \"*.{js,cjs,mjs,json}\" ",
    "format:fix": "prettier --config ../../.prettierrc.json  --ignore-path ../../.prettierignore  --write \"src/**/*.{ts,cts,mts}\" \"*.{js,cjs,mjs,json}\" ",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix --fix-type [problem,suggestion]",
    "pack": "npm pack 2>&1",
    "prepack": "npm run build",
    "test": "vitest --run"
  },
  "keywords": [],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "description": "The runtime for GenAIScript, providing the core functionality and APIs for executing scripts.",
  "repository": "github:microsoft/genaiscript",
  "bugs": {
    "url": "https://github.com/microsoft/genaiscript/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "tshy": {
    "dialects": [
      "esm",
      "commonjs"
    ],
    "project": "./tsconfig.src.json",
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json"
    },
    "selfLink": false
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  }
}
