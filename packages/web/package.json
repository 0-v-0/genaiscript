    "nanoid": "^5.1.5",
    "pretty-bytes": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-mermaid": "^3.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^1.3.1",
    "remark-math": "^6.0.0",
    "remark-mermaid": "^0.2.0",
    "victory": "^37.3.6",
