{
  "name": "@genaiscript/web",
  "version": "2.0.12",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run clean && node build.mjs",
    "clean": "rimraf dist",
    "format:check": "prettier --config ../../.prettierrc.json --ignore-path ../../.prettierignore --check  \"src/**/*.{ts,cts,mts,tsx,css}\" \"*.{js,cjs,mjs,json}\" ",
    "format:fix": "prettier --config ../../.prettierrc.json  --ignore-path ../../.prettierignore  --write \"src/**/*.{ts,cts,mts,tsx,css}\" \"*.{js,cjs,mjs,json}\" ",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix --fix-type [problem,suggestion]",
    "watch": "node --watch --watch-path=./src --watch-path=../core/src build.mjs --dispatch-progress",
    "serve": "serve -n .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
