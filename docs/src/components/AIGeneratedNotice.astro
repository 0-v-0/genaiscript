---
import ContentNotice from "./ContentNotice.astro";
const { starlightRoute } = Astro.locals;
const isLocalized = starlightRoute.lang !== "en";
---
{isLocalized ? (
  <ContentNotice
    icon="information"
    label="AI generated translation."
    link="https://github.com/microsoft/genaiscript/blob/dev/packages/sample/genaisrc/mdtranslator.genai.mts"
  />
) : null}
