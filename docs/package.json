{
  "name": "docs",
  "type": "module",
  "private": true,
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "install:playwright": " pnpm exec playwright install --with-deps chromium",
    "postinstall": "pnpm install:playwright",
    "dev": "astro telemetry disable && astro dev --host",
    "start": "astro dev --host",
    "check": "astro check",
    "build": "astro build",
    "build:asw": "rm -Rf distasw && mkdir distasw && touch distasw/index.html && mkdir distasw/genaiscript && cp -r dist/* distasw/genaiscript && node fixllms.mjs",
    "lint:fix": "prettier --write \"src/content/**/*.{md,mdx}\"",
    "preview": "astro preview",
    "genai:test": "node ../packages/cli/dist/src/index.js test src/**/*.md",
    "genai:frontmatter": "node ../packages/cli/dist/src/index.js run frontmatter \"src/**/*.{md,mdx}\" --apply-edits",
    "genai:technical": "for file in \"src/**/*.md\"; do\nnode ../packages/cli/dist/src/index.js run technical \"$file\" --apply-edits\ndone",
    "genai:alt-text": "node scripts/image-alt-text.mjs",
    "disk:check": "du -h --max-depth=2 | sort -hr | head -n 10",
    "clean": "rm -Rf dist && rm -Rf distasw && rm -Rf public/slides && rm -Rf .genaiscript"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/starlight": "^0.34.4",
    "astro": "^5.10.1",
    "astro-embed": "^0.9.0",
    "rehype-mermaid": "^3.0.0",
    "starlight-blog": "^0.24.0",
    "starlight-links-validator": "^0.17.0",
    "starlight-llms-txt": "^0.5.1",
    "starlight-package-managers": "^0.11.0",
    "zx": "catalog:"
  },
  "devDependencies": {
    "prettier": "catalog:"
  },
  "resolutions": {
    "marked": "15.0.8"
  }
}
