{
  "name": "action-poem-action",
  "version": "1.141.0",
  "author": "microsoft",
  "license": "MIT",
  "description": "This is the description",
  "dependencies": {
    "genaiscript": "^1.141.0"
  },
  "scripts": {
    "docker:build": "docker build -t action-poem-action .",
    "docker:start": "docker run -e GITHUB_TOKEN action-poem-action",
    "lint": "npx --yes prettier --write genaisrc/",
    "typecheck": "genaiscript scripts compile",
    "configure": "genaiscript action configure action-poem --out .",
    "start": "GITHUB_TOKEN=$INPUT_GITHUB_TOKEN genaiscript run action-poem --github-action"
  }
}