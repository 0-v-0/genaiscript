{
  "name": "@genaiscript/eslint-plugin-genaiscript",
  "private": true,
  "version": "2.2.1",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.d.*ts.map",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "catalog:",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-promise": "^7.2.1"
  },
  "dependencies": {
    "@eslint/js": "catalog:",
    "eslint-config-prettier": "^10.1.5",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.0.0",
    "@typescript-eslint/rule-tester": "^8.35.1",
    "@typescript-eslint/utils": "^8.35.1",
    "@vitest/coverage-istanbul": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "tshy": "catalog:",
    "vitest": "catalog:"
  },
  "scripts": {
    "build": "npm run clean && tshy",
    "clean": "rimraf dist",
    "format:check": "prettier --config ../../.prettierrc.json --ignore-path ../../.prettierignore --check  \"src/**/*.{ts,cts,mts}\" \"*.{js,cjs,mjs,json}\" ",
    "format:fix": "prettier --config ../../.prettierrc.json  --ignore-path ../../.prettierignore  --write \"src/**/*.{ts,cts,mts}\" \"*.{js,cjs,mjs,json}\" ",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix --fix-type [problem,suggestion]",
    "pack": "npm pack 2>&1",
    "prepack": "npm run build",
    "test": "vitest --run"
  },
  "keywords": [],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "description": "The runtime for GenAIScript, providing the core functionality and APIs for executing scripts.",
  "repository": "github:microsoft/genaiscript",
  "bugs": {
    "url": "https://github.com/microsoft/genaiscript/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "tshy": {
    "dialects": [
      "esm",
      "commonjs"
    ],
    "project": "./tsconfig.src.json",
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json"
    },
    "selfLink": false
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  }
}
